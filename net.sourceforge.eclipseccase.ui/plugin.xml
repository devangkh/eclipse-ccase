   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Clearcase"
            description="Clearcase actions"
            id="net.sourceforge.eclipseccase.category">
      </category>
      <command
            name="Associate"
            description="Associates the selected project with the clearcase plugin"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.associate">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.associate"
            scope="org.eclipse.ui.globalScope"
            keySequence="Alt+C Alt+S"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Dissociate"
            description="Dissociates the selected project from the clearcase plugin"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.dissociate">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.dissociate"
            scope="org.eclipse.ui.globalScope"
            keySequence="Alt+C Alt+E"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Uncheckout"
            description="Cancels the checkout for the selected resources"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.uncheckout">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.uncheckout"
            scope="org.eclipse.ui.globalScope"
            keySequence="Alt+C Alt+U"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Checkin"
            description="Checks in th selected resources"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.checkin">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.checkin"
            keySequence="Alt+C Alt+I"
            scope="org.eclipse.ui.globalScope"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Refresh"
            description="Refreshes eclipse&apos;s cache of the resource&apos;s clearcase state"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.refresh">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.refresh"
            scope="org.eclipse.ui.globalScope"
            keySequence="Alt+C Alt+R"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Refresh Quick"
            description="Quickly refreshes eclipse&apos;s cache of the resource&apos;s clearcase state"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.refreshquick">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.refreshquick"
            string="org.eclipse.ui.globalScope"
            scope="org.eclipse.ui.globalScope"
            keySequence="Alt+C Alt+Q"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Checkout"
            description="Checks out the selected resources"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.checkout">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.checkout"
            string="org.eclipse.ui.globalScope"
            scope="org.eclipse.ui.globalScope"
            keySequence="Alt+C Alt+O"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Update"
            description="Updates the seletced resources (Static view only)"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.update">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.update"
            keySequence="Alt+C Alt+T"
            scope="org.eclipse.ui.globalScope"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Add"
            description="Adds the selected resources to clearcase"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.add">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.add"
            keySequence="Alt+C Alt+A"
            scope="org.eclipse.ui.globalScope"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Delete"
            description="Removes the selected resources from clearcase"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.delete">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.delete"
            keySequence="Alt+C Alt+D"
            scope="org.eclipse.ui.globalScope"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Version Tree"
            description="Opens the clearcase version tree browser on the slected resources (external tool)"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.versiontree">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.versiontree"
            keySequence="Alt+C Alt+V"
            scope="org.eclipse.ui.globalScope"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Update (External)"
            description="Updates the selected resources (external tool)"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.externalupdate">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.externalupdate"
            keySequence="Alt+C Alt+X"
            scope="org.eclipse.ui.globalScope"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="History"
            description="Opens the clearcase hostory browser on the selected resources (external tool)"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.history">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.history"
            keySequence="Alt+C Alt+H"
            scope="org.eclipse.ui.globalScope"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Compare with predecessor"
            description="Opens the clearcase compare with predecessor tool on the selected resources (external tool)"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.comparepredecessor">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.comparepredecessor"
            keySequence="Alt+C Alt+C"
            scope="org.eclipse.ui.globalScope"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Find Checkouts"
            description="Opens the clearcase find checkouts tool on the selected resource (external tool)"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.findcheckouts">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.findcheckouts"
            keySequence="Alt+C Alt+F"
            scope="org.eclipse.ui.globalScope"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Cancel Pending Refreshes"
            description="Cancels all pending refresh state jobs"
            categoryId="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.ui.actionset.cancelRefreshes">
      </command>
      <keyBinding
            commandId="net.sourceforge.eclipseccase.ui.actionset.cancelRefreshes"
            keySequence="Alt+C Alt+Z"
            scope="org.eclipse.ui.globalScope"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
