               class="net.sourceforge.eclipseccase.ui.actions.VersionTreeAction"
               menubarPath="team.main/group2"
               id="net.sourceforge.eclipseccase.popups.versiontree">
         </action>
         <action
               label="Update (External)"
               class="net.sourceforge.eclipseccase.ui.actions.ExternalUpdateAction"
               menubarPath="team.main/group2"
               id="net.sourceforge.eclipseccase.popups.externalupdate">
