               class="net.sourceforge.eclipseccase.ui.actions.AddToClearcaseAction"
               menubarPath="team.main/group1"
               id="net.sourceforge.eclipseccase.popups.add">
         </action>
         <action
               label="Delete"
               class="net.sourceforge.eclipseccase.ui.actions.DeleteAction"
               menubarPath="team.main/group1"
               id="net.sourceforge.eclipseccase.popups.delete">
