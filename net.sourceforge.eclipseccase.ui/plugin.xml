               menubarPath="team.main/group2"
               id="net.sourceforge.eclipseccase.popups.history">
         </action>
         <action
               label="Compare with predecessor"
               class="net.sourceforge.eclipseccase.ui.actions.CompareWithPredecessorAction"
               menubarPath="team.main/group2"
               id="net.sourceforge.eclipseccase.popups.comparepredecessor">
         </action>
         <action
               label="Predecessor"
               class="net.sourceforge.eclipseccase.ui.actions.CompareWithPredecessorAction"
               menubarPath="compareWithMenu/compareWithGroup"
               id="net.sourceforge.eclipseccase.popups.comparepredecessor2">
         </action>
         <action
               label="Find checkouts"
               class="net.sourceforge.eclipseccase.ui.actions.FindCheckOutsAction"
               menubarPath="team.main/group2"
               id="net.sourceforge.eclipseccase.popups.findcheckouts">
            <enablement>
               <and>
                  <pluginState
                        value="activated"
                        id="net.sourceforge.eclipseccase">
                  </pluginState>
                  <systemProperty
                        name="osgi.os"
                        value="win32">
                  </systemProperty>
               </and>
            </enablement>
         </action>
         <action
               definitionId="net.sourceforge.eclipseccase.ui.actionset.cancelRefreshes"
               label="Cancel pending refreshes"
               class="net.sourceforge.eclipseccase.ui.actions.CancelPendingRefreshesAction"
               menubarPath="team.main/group3"
               id="net.sourceforge.eclipseccase.popups.cancelRefreshes">
         </action>
      </objectContribution>
   </extension>
