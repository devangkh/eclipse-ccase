               label="Activate clearcase plugin"
               class="net.sourceforge.eclipseccase.actions.ActivatePluginAction"
               menubarPath="team.main/projectGroup"
               id="net.sourceforge.eclipseccase.popups.activate">
            <enablement>
               <not>
                  <pluginState
                        value="activated"
                        id="net.sourceforge.eclipseccase">
                  </pluginState>
               </not>
            </enablement>
         </action>
      </objectContribution>
   </extension>
