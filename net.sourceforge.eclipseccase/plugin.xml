   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Clearcase"
            description="Clearcase actions"
            id="net.sourceforge.eclipseccase.category">
      </category>
      <command
            name="Associate"
            description="Associates the selected project with the clearcase plugin"
            category="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.actionset.associate">
      </command>
      <keyBinding
            string="Alt+C Alt+S"
            scope="org.eclipse.ui.globalScope"
            command="net.sourceforge.eclipseccase.actionset.associate"
            configuration="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
            name="Dissociate"
            description="Dissociates the selected project from the clearcase plugin"
            category="net.sourceforge.eclipseccase.category"
            id="net.sourceforge.eclipseccase.actionset.dissociate">
      </command>
      <keyBinding
            string="Alt+C Alt+E"
            scope="org.eclipse.ui.globalScope"
            command="net.sourceforge.eclipseccase.actionset.dissociate"
            configuration="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <command
